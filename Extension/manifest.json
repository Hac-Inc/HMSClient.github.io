{
        "name": "HMS Client",
        "version": "1.0",
        "description": "Chrome Extension for the HMS Utlity Mod, which allows the client to inject automatically",
        "background": {
                "scripts": ["injector.js"],
                "persistent": false
        },
        "browser_action": {
                "default_icon": "HMSClientLogo.png"
        },
        "icons": {
                "128": "HMSClientLogo.png"
        },
        "permissions": [
                "https://*/*",
                "http://*/*",
                "tabs"
        ],
        "manifest_version": 2
}